<?xml version="1.0" encoding="utf-8" ?>
<appSettings>

  <!--Added to protect sensitive data from being committed to Source Control.-->
  <!--Because of the file attribute on the appSettings in the Web.config, this section will be merged together with the appSettings section there at runtime. For the file attribute to work, this config file should only contain information about appSettings.-->

  <!--Replace your.domain.com below with your actual domain and extension-->
  <add key="EmailClient" value="mail.zaksisler.com"/>

  <!--Replace mail@yourdomain.com with email user created on your hosting account-->
  <add key="EmailUser" value="administrator@zaksisler.com"/>

  <!--Replace password for your email user's password-->
  <add key="EmailPass" value="P@ssw0rd"/>

  <!--Prelace mail@domain.com with your personal email address-->
  <add key="EmailTo" value="zakarysisler@outlook.com"/>

</appSettings>